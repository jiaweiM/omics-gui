<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<fx:root type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <javafx.scene.layout.HBox alignment="CENTER_LEFT" spacing="6.0" AnchorPane.leftAnchor="6.0" AnchorPane.topAnchor="6.0">
          <Label text="Percolator executable:" />
          <TextField fx:id="percolatorPathField" minWidth="250.0" />
          <Button fx:id="chooseFileButton" mnemonicParsing="false" onAction="#choose" />
      </javafx.scene.layout.HBox>
   </children>
</fx:root>
